stock GetClosestPlayer(p1)
{
	new Float:dis,Float:dis2,player;
	player = INVALID_PLAYER_ID;
	dis = 99999.99;
	foreach(Player,x)
	{
		if(IsPlayerConnected(x))
		{
			if(x != p1)
			{
				dis2 = GetDistanceBetweenPlayers(x,p1);
				if(dis2 < dis && dis2 != -1.00) dis = dis2,player = x;
			}
		}
	}
	return player;
}
stock PayDay()
{
	WarBeen[0] = 0;
	WarBeen[1] = 0;
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		if(GZInfo[i][gFrakVlad] == 18) TotalGzR++;
		if(GZInfo[i][gFrakVlad] == 17) TotalGzA++;
		if(GZInfo[i][gFrakVlad] == 15) TotalGzG++;
		if(GZInfo[i][gFrakVlad] == 13) TotalGzV++;
		if(GZInfo[i][gFrakVlad] == 12) TotalGzB++;
	}
	FracBank[0][fGrove] += TotalGzG*300;
	FracBank[0][fBallas] += TotalGzB*300;
	FracBank[0][fVagos] += TotalGzV*300;
	FracBank[0][fAztek] += TotalGzA*300;
	FracBank[0][fRifa] += TotalGzR*300;
	capture_number = 0;
	Update3DTextGangs();
	ClearTotalGz();
	/////////
	foreach(Player,i)
	{
		if(PlayerInfo[i][pWanted] >= 1)
		{
			WantedLevel[i] -= 1;
			PlayerInfo[i][pWanted] -=1;
			SetPlayerWantedLevel(i, PlayerInfo[i][pWanted]);
		}
		new exp = PlayerInfo[i][pExp];
		new nxtlevel = PlayerInfo[i][pLevel]+1;
		new expamount = nxtlevel*levelexp;
		if(exp >= expamount)
		{
			PlayerInfo[i][pLevel] += 1;
			PlayerInfo[i][pExp] = 0;
			SendClientMessage(i,COLOR_WHITE,"Ваш игровой lvl повысился.");
			SetPlayerScore(i, PlayerInfo[i][pLevel]);
		}
		new moneys = 1+random(200000);
		switch(PlayerInfo[i][pMRInections])
		{
		case 1,10,21,25:{ PlayerInfo[i][pPayCheck] += 2000*PlayerInfo[i][pRKInections]+moneys; } // ЗП полиции, 2к умноженое на ранг + рандомные цифры
		case 3,19,26:{ PlayerInfo[i][pPayCheck] += 1500*PlayerInfo[i][pRKInections]+moneys; } // Зп армии, 1.5к умноженое на ранг + рандомные цифры
		case 2,27,16,17,18:{ PlayerInfo[i][pPayCheck] += 3000*PlayerInfo[i][pRKInections]+moneys; } // Зп ФБР, 3к умноженое на ранг + рандомные цифры
		case 7,12,13,15:{ PlayerInfo[i][pPayCheck] += 4000*PlayerInfo[i][pRKInections]+moneys; } // Зп Мэрия, 4к умноженое на ранг + рандомные цифры
		case 4,22:{ PlayerInfo[i][pPayCheck] += 50000*PlayerInfo[i][pRKInections]+moneys; } // Зп МинЗдрав, 4к умноженое на ранг + рандомные цифры
		case 11,8:{ PlayerInfo[i][pPayCheck] += 50000*PlayerInfo[i][pRKInections]+moneys; } // Зп инструкторов, 4к умноженое на ранг + рандомные цифры
		case 5,6,14:{ PlayerInfo[i][pPayCheck] += 5000*PlayerInfo[i][pRKInections]+moneys; } // Зп инструкторов, 4к умноженое на ранг + рандомные цифры
		}
		PlayerInfo[i][pExp]++;
		new checks = PlayerInfo[i][pPayCheck];
		if(PlayerInfo[i][pDRInections] >=1) GameTextForPlayer(i, "PayDay", 5000, 1);
		else GameTextForPlayer(i, "~w~PayDay", 5000, 1);
		//-------------------------- Работам -------------------
		if(PlayerInfo[i][pDRInections] >=1) SendClientMessage(i, 0xeac700aa,  "--------===[ VIP КЛИЕНТ БАНКА SA ]===-------");
		else SendClientMessage(i, COLOR_GRAD1, "----------===[ КЛИЕНТ БАНКА SA ]===---------");
		SendMes(i,0xFF8C37AA,"***** Налог государству: %d вирт *****", nalog);
		PlayerInfo[i][pBank] -= nalog;
		kazna +=nalog;
		if(PlayerInfo[i][pTenPayDay] > 0)
		{
		    PlayerInfo[i][pExp] += 9;
		    SendClientMessage(i, COLOR_RED, "Вы получили 10 EXP! Спасибо за поддержку.");
		}
		if(PlayerInfo[i][pHelper] == 0)
		{
			if(PlayerInfo[i][pPayDayHad] == 30)
			{
			    PlayerInfo[i][pHelper] += 1;
			    SendClientMessage(i, COLOR_RED, "Вы отыграли 30 часов и получили права хелпера.");
			    SendClientMessage(i, COLOR_RED, "Введите /hhelp чтобы посмотреть команды.");
			}
		}
		if(PlayerInfo[i][pDRInections] >=1)
		{
			SendMes(i,0xFF8C37AA," Счет за телефон: -%d вирт",PlayerInfo[i][pMobile]);
			SendClientMessage(i, 0xeac700aa, " ");
			SendMes(i,0xFF8C37AA," Зарплата: %d вирт", checks);
			PlayerInfo[i][pBank] += checks;
			SendMes(i,0xeac700aa," Текущий баланс: %d вирт", PlayerInfo[i][pBank]);
			PlayerInfo[i][pZakonp] += 1;
			PlayerInfo[i][pBank] += PlayerInfo[i][pMobile];
			PlayerInfo[i][pMobile] = 0;
			PlayerInfo[i][pVipRespect]++;
			if(PlayerInfo[i][pVipRespect] == 3)
			{
