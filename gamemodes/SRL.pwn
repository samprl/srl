stock AdminLevel(playerid)
{
	if(!PlayerInfo[playerid][pAdmInections]) return 1;
	if(PlayerInfo[playerid][pAdmInections] >= 15) return SendClientMessage(playerid, -1, "У Вас максимальный уровень админ-прав");
	if(PlayerInfo[playerid][pAdmInections] < 9)
	{
		if(PlayerInfo[playerid][pRub] < 50) return SendClientMessage(playerid, -1, "Для повышение админ-уровня Вам требуется 50 рублей на балансе");
		PlayerInfo[playerid][pAdmInections] ++;
		PlayerInfo[playerid][pRub] -= 50;
		SendClientMessage(playerid, -1, "Введите команду /alogin по новой, что бы Ваш уровень обновился.");
		return 1;
	}
	else if(PlayerInfo[playerid][pAdmInections] >= 9)
	{
		if(PlayerInfo[playerid][pRub] < 100) return SendClientMessage(playerid, -1, "Для повышение админ-уровня Вам требуется 100 рублей на балансе");
		PlayerInfo[playerid][pAdmInections] ++;
		PlayerInfo[playerid][pRub] -= 100;
		SendClientMessage(playerid, -1, "Введите команду /alogin по новой, что бы Ваш уровень обновился.");
		return 1;
	}
	return 1;
}
