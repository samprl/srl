{
	if(!PlayerInfo[playerid][pHelper]) return 1;
	if(PlayerInfo[playerid][pHelper] >= 8) return SendClientMessage(playerid, -1, "У Вас максимальный уровень прав хелпера.");
	if(PlayerInfo[playerid][pHelper] <= 4)
	{
		if(PlayerInfo[playerid][pRub] < 12) return SendClientMessage(playerid, -1, "Для повышения прав хелпера Вам потребуется 12 рублей.");
		PlayerInfo[playerid][pHelper] ++;
		PlayerInfo[playerid][pRub] -= 12;
		SendMes(playerid, COLOR_GREEN, "Вы успешно повысили свой уровень хелперки до %d.", PlayerInfo[playerid][pHelper]);
		SendClientMessage(playerid, -1, "Просмотреть новые команды Вы можете введя команду ''/hhelp''");
		return 1;
	}
	else if(PlayerInfo[playerid][pHelper] > 4)
	{
		if(PlayerInfo[playerid][pRub] < 15) return SendClientMessage(playerid, -1, ""#NET"Для повышения прав хелпера Вам потребуется 15 рублей.");
		PlayerInfo[playerid][pHelper] ++;
		PlayerInfo[playerid][pRub] -= 15;
		SendMes(playerid, COLOR_GREEN, "Вы успешно повысили свой уровень хелперки до %d.", PlayerInfo[playerid][pHelper]);
		SendClientMessage(playerid, -1, "Просмотреть новые команды Вы можете введя команду ''/hhelp''");
		return 1;
	}
	return 1;
}
