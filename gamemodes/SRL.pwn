	if(IsPlayerConnected(playerid))
	{
		if(PlayerInfo[playerid][pMRInections] == 7) return true;
		else if(PlayerInfo[playerid][pLRInections] == 7) return true;
	}
	return false;
}
stock IsACop(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		new leader = PlayerInfo[playerid][pLRInections];
		new member = PlayerInfo[playerid][pMRInections];
		if(member==1 || member==2 || member==21 || member ==25 || member==10 || member==26 || member == 3) return true;
		if(leader==21 || leader==2 || leader == 10 || leader == 1 || leader==25 || leader==26 || leader == 3) return true;
	}
	return false;
}
stock IsAArm(playerid)
{
	if(IsPlayerConnected(playerid))
	{
		new leader = PlayerInfo[playerid][pLRInections];
		new member = PlayerInfo[playerid][pMRInections];
		if(member==3 || member==19 || member==28 || member==1 || member==2 || member==21 || member==10) return true;
		else if(leader==3 || leader==19) return true;
	}
	return false;
}

