publics: UpdateSpeedometr(playerid)
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		new vehicleid = GetPlayerVehicleID(playerid);
		new str[64],str3[64],str4[64], Float:vehhel, vehh[20];
		GetVehicleHealth(GetPlayerVehicleID(playerid), vehhel);
		if(vehhel <= 500) vehh = "~r~";
		else vehh = "";
		if(Fuell[vehicleid] > 0) format(str4, sizeof(str4), "%.0f",Fuell[vehicleid]);
		format(str, sizeof(str), " %d KM/H",SpeedVehicle(playerid));
		if(vehhel <= 300) format(str3, sizeof(str3),"%s30%",vehh); else format(str3, sizeof(str3),"%s%.0f%",vehh,vehhel / 10);
		if(SpeedVehicle(playerid) !=0)
		{
			Fuell[vehicleid] -= 0.003;
			if(GetPlayerVehicleID(playerid) == caridhouse[playerid]) { PlayerInfo[playerid][pFuelcar] -= 0.003; }
		}
		new carid = GetPlayerVehicleID(playerid);
		if(Fuell[vehicleid] <= 0)
		{
			GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(GetPlayerVehicleID(playerid),VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
			zavodis[playerid] = 0;
		}
		if(SuperGt(carid))
		{
			if(SpeedVehicle(playerid) !=0) { Fuell[vehicleid] -= 0.005; if(GetPlayerVehicleID(playerid) == caridhouse[playerid]) PlayerInfo[playerid][pFuelcar] -=0.005; }
		}
		TextDrawSetString(FuelShow[playerid],str4);
		TextDrawSetString(HealthShow[playerid],str3);
		TextDrawSetString(SpeedShow[playerid],str);
	}
	return false;
}
