publics: OffBan(playerid, tmp[], days, reason[])
{
	new year, month,day,mysql_query[200];
	getdate(year, month, day);
	new curdate = getdate(year,month,day),rows, fields;
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[A] Игрок %s не найден в базе данных.",tmp);
	if(anti[playerid]) return SendClientMessage(playerid, COLOR_LIGHTRED, "Ваши адм. действия заблокированы!");
	format(mysql_query, sizeof(mysql_query), "UPDATE `accounts` SET `pLocked2` = '1',`pBanData1` = '%d', `pBanTime1` = '%d' WHERE `Name` = '%s'",(curdate+days),curdate, tmp);
	mysql_function_query(connects, mysql_query, false, "", "");
	/*format(stringer, sizeof(stringer), "INSERT INTO `banlog` (`Name`, `NameAdmin`, `Day`, `Date`, `Text`) VALUES ('%s','%s','%d','%s','%s')",tmp,PlayerInfo[playerid][pNames],days,curdate,reason);
	mysql_function_query(connects, stringer, false, "", "");*/
	format(stringer, sizeof(stringer), "Администратор %s[%d] забанил %s в оффлайне на %d дней. Причина: %s", PlayerInfo[playerid][pNames], playerid, tmp, days, reason);
	SendClientMessageToAll(COLOR_LIGHTRED, stringer);
	antiwarn[playerid] += 15;
	if(antiwarn[playerid] >= 30 && anti[playerid] == 0)
	{
		anti[playerid] = 1;
		antiwarn[playerid] = 300;
		SendClientMessage(playerid, COLOR_LIGHTRED, "[A] Вы словили блок на наказание сроком 5 минут.");
	}
	return true;
}

