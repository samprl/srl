stock ConvertSeconds(time)
{
	new string[128];
	if(time < 100000) format(string, sizeof(string), "%d секунд", time);
	{
		new seconds;
		seconds = time % 100000;
		format(string, sizeof(string), "%d секунд",seconds);
	}
	return string;
}
stock ReplaceID(inputtext[])
{
    for(new i = strfind(inputtext, "@", true), a = 0, tmp[4]; i != -1; i = strfind(inputtext, "@", true, i))
	{
		++i, a = 0;
  		while(48 <= inputtext[i] <= 57 && a != 4) tmp[a++] = inputtext[i++];
		if(tmp[0] == EOS) continue;
		if(!strlen(tmp)) continue;
		strdel(inputtext, i - a - 1, i);
		strins(inputtext, GetName(strval(tmp)), i - a - 1, 128);
		return true;
	}
	return 1;
}
